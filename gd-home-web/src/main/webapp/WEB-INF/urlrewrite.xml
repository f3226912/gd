<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite use-query-string ="false">

	<rule>
		<from>/nst/nst.apk</from>
		<to>/nst/nst.html</to>
	</rule>
	<!-- <rule>
		<from>/nsy/nsy.apk</from>
		<to>/nsy/nsy.html</to>
	</rule> -->
	<rule>
		<from>/baishazhou/market.html</from>
		<to>/product/1/list</to>
	</rule>
	<rule>
		<from>/baishazhou/market/([0-9]+)-([0-9]+).html</from>
		<to>/product/list/query/1/$1-$2</to>
	</rule>
	<rule>
		<from>/yulin/market.html</from>
		<to>/product/2/list</to>
	</rule>
	<rule>
		<from>/yulin/market/([0-9]+)-([0-9]+).html*</from>
		<to>/product/list/query/2/$1-$2</to>
	</rule>
	<rule>
		<from>/(.*)/market/([0-9]+).html</from>
		<to>/product/list/$2</to>
	</rule>
	<rule>
		<from>/baishazhou/business.html</from>
		<to>/business/1/list</to>
	</rule>
	<rule>
		<from>/baishazhou/business/([0-9]+)-([0-9]+).html*</from>
		<to>/business/list/query/1/$1-$2</to>
	</rule>
	<rule>
		<from>/yulin/business.html</from>
		<to>/business/2/list</to>
	</rule>
	<rule>
		<from>/yulin/business/([0-9]+)-([0-9]+).html*</from>
		<to>/business/list/query/2/$1-$2</to>
	</rule>
	<rule>
		<from>/(.*)/business/([0-9]+).html</from>
		<to>/business/list/$2</to>
	</rule>
	
	<rule>
		<from>/market/([0-9]+).html</from>
		<to>/product/$1</to>
	</rule>
	<rule>
		<from>/(.*)/business/shop/([0-9]+).html</from>
		<to>/business/shop/$2</to>
	</rule>
	<rule>
		<from>/(.*)/business/shop/([0-9]+)/intro.html</from>
		<to>/business/shop/$2/intro</to>
	</rule>
	<rule>
		<from>/(.*)/business/shop/([0-9]+)/supply.html</from>
		<to>/business/shop/$2/supply</to>
	</rule>
	<rule>
		<from>/(.*)/business/shop/([0-9]+)/supply/([0-9]+).html</from>
		<to>/business/shop/$2/supply/page/$3</to>
	</rule>
	<rule>
		<from>/(.*)/business/shop/([0-9]+)/contact.html</from>
		<to>/business/shop/$2/contact</to>
	</rule>
	
	<rule>
		<from>/baishazhou/test.html</from>
		<to>/test/1/index</to>
	</rule>
	<rule>
		<from>/yulin/test.html</from>
		<to>/test/2/index</to>
	</rule>
	<rule>
		<from>/test.html</from>
		<to>/test/index</to>
	</rule>
	<rule>
		<from>/test/([0-9]+).html</from>
		<to>/test/index/page/$1</to>
	</rule>
	<rule>
		<from>/logistics.html</from>
		<to>/logistics/index</to>
	</rule>
	<rule>
		<from>/logistics/0-([0-9]+).html*</from>
		<to>/logistics/index/query/$1</to>
	</rule>
	<rule>
		<from>/logistics/1-([0-9]+).html*</from>
		<to>/logistics/queryGoodsList/query/$1</to>
	</rule>
	<rule>
		<from>/finance.html</from>
		<to>/finance/index</to>
	</rule>
	<rule>
		<from>/help.html</from>
		<to>/help/index</to>
	</rule>
	<rule>
		<from>/service.html</from>
		<to>/service/index</to>
	</rule>
	<rule>
		<from>/about.html</from>
		<to>/about/index</to>
	</rule>
	<rule>
		<from>/contactus.html</from>
		<to>/about/contactus</to>
	</rule>
	<rule>
		<from>/sitemap.html</from>
		<to>/siteMap</to>
	</rule>
</urlrewrite>