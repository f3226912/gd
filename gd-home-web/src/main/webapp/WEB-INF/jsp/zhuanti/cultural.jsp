<script>
	;(function($, window){
		var topArr = [],
			winHeight = 400,
			i;
		for (i=1; i<6; i++){
			topArr.push($("#cul"+i).offset().top-winHeight);
		}
		var len = topArr.length;
		$(window).bind("scroll",function(){
			var scrollTop = $(window).scrollTop(),
				i;
			if(scrollTop > winHeight){
				$("#nav").show();
				for (i=1; i<=len; i++){
					if(topArr[i]){
						if(topArr[i-1] <= scrollTop && topArr[i] > scrollTop){ //判断滚动条位置
							$("#nav a").removeClass("active"); //清除active类
							$("#cul"+i+i).addClass("active");	//给当前导航加active类
						}
					}else{
						if(topArr[len-1] <= scrollTop){
							$("#nav a").removeClass("active"); //清除active类
							$("#cul"+i+i).addClass("active");
						}
					}
				}
			}else{
				$("#nav").hide();
			}
		});
		$("#nav").on("click", "a", function(){
			$("#nav a").removeClass("active");
			$(this).addClass("active");
		});
	})(jQuery, window);
</script>
